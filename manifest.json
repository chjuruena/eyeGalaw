{
  "manifest_version": 2,

  "name": "eyeGalaw",
  "description": "eyegalaw",
  "version": "1.0",
  
  "background": {
    "scripts": ["background.js", "thirdParty/jquery-3.1.1.min.js", 
    "thirdParty/jquery-ui.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
    // "tabs", "http://*/*", "https://*/*"
    // "https://ajax.googleapis.com/"
  ]
}
