{
  "manifest_version": 2,

  "name": "eyeGalaw",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab"
    // "tabs", "http://*/*", "https://*/*"
    // "https://ajax.googleapis.com/"
  ]
}
